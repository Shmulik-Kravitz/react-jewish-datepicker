{
  "name": "jewish-dates-core",
  "version": "2.0.13",
  "description": "Jewish dates core",
  "main": "dist/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.js",
        "types": "./lib/index.d.ts"
      },
      "import": {
        "default": "./dist/esm/index.js",
        "types": "./lib/index.d.ts"
      }
      "require": "./dist/index.js",
      "import": "./dist/mjs/index.js"
    }
  },
  "files": [
    "dist",
    "lib",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Shmulik-Kravitz/react-jewish-datepicker"
  },
  "homepage": "https://react-jewish-datepicker.js.org/",
  "author": "Shmulik Kravitz <29735690+Shmulik-Kravitz@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn g:start",
    "dev": "yarn g:dev",
    "build": "yarn g:build",
    "test": "yarn g:test",
    "inc": "yarn version patch",
    "pub": "npm publish --provenance --access public",
    "np": "np patch"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "jewish-date": "^2.0.12"
  },
  "devDependencies": {
    "esbuild": "^0.24.0",
    "typescript": "^5.6.2"
  },
  "keywords": [
    "jewish",
    "hebrew",
    "date",
    "luach",
    "calendar"
  ]
}
